  <script>
    function load_post() {
      var post_title = "{{post_title}}";
      var post_family = "{{post_family}}";
      var post_id = "{{post_id}}";
      post_title = post_title.split(",");
      post_family = post_family.split(",");
      post_id = post_id.split(",");
      const root_mondo = document.getElementById('root-mondo');
      const root_storia = document.getElementById('root-storia');
      const root_classi = document.getElementById('root-classi');
      const root_razze = document.getElementById('root-razze');
      const root_homebrew = document.getElementById('root-homebrew');
      const root_help = document.getElementById('root-help');
      const root_altro = document.getElementById('root-altro');
      for (var i = 0; i < post_id.length; i++) {
        var template = `<a href="/post/${post_id[i]}">
                          <div class="u-clearfix u-rich-text u-text">
                            <h5 class="u-accordion-link-text">â€¢ ${post_title[i]}</h5>
                            <hr align="left" size="5" color="#4c7397" noshade>
                          </div>
                        </a>`;
        if (post_family[i] == "mondo") {
          root_mondo.insertAdjacentHTML('afterbegin', template);
        } else if (post_family[i] == "storia") {
          root_storia.insertAdjacentHTML('afterbegin', template);
        } else if (post_family[i] == "classi") {
          root_classi.insertAdjacentHTML('afterbegin', template);
        } else if (post_family[i] == "razze") {
          root_razze.insertAdjacentHTML('afterbegin', template);
        } else if (post_family[i] == "homebrew") {
          root_homebrew.insertAdjacentHTML('afterbegin', template);
        } else if (post_family[i] == "help") {
          root_help.insertAdjacentHTML('afterbegin', template);
        } else if (post_family[i] == "altro") {
          root_altro.insertAdjacentHTML('afterbegin', template);
        }
      }
    }
    load_post();
  </script>
